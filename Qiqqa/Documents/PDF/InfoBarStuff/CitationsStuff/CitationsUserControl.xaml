<UserControl
    x:Class="Qiqqa.Documents.PDF.InfoBarStuff.CitationsStuff.CitationsUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel>
        <TextBlock>
            <Bold>
                Outbound
            </Bold>
        </TextBlock>
        <StackPanel Name="DocsPanel_Outbound" />
        <DockPanel>
            <Image
                Name="ImageRefresh"
                Height="16"
                DockPanel.Dock="Right" />
            <TextBlock>
                <Bold>
                    Inbound
                </Bold>
            </TextBlock>
        </DockPanel>
        <StackPanel Name="DocsPanel_Inbound" />
    </StackPanel>
</UserControl>