<UserControl
    x:Class="Qiqqa.Documents.PDF.PDFControls.MetadataControls.TagsControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Input.Toolkit"
    xmlns:tag="clr-namespace:Qiqqa.Common.TagManagement">
    <Grid>
        <tag:TagEditorControl
            x:Name="ObjTagEditorControl"
            TagsBundle="{Binding Tags, Mode=TwoWay}"
            TagsTitleVisibility="Collapsed" />
    </Grid>
</UserControl>